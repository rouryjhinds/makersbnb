<h1 class="text-center">List a new space</h1>

<form action="/space" method="post" enctype="multipart/form-data" class="big_box text-center">
  <label for="name">
    Title <br>
    <input name="name" type="text">
  </label><br />
  <label for="description">
    Description<br>
    <textarea cols="50" rows="4" name="description"></textarea>
  </label><br />
  <label for="price">
    Price per Night <br>
    <input name="price" type="number">
  </label><br />
  <input type="submit" value="Add">
</form>
